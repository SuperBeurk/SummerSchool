@startuml
(*) --> "No Game"
"No Game" --> [evOnePlayer] "Local"
"No Game" --> [evParameters] "Parameters"
"No Game" --> [evTwoPlayers] "Online"

"Local" --> [evEndGame] "End Game"
"Parameters" --> [evLeaveParam] "No Game"
"Online" --> [evEndGame] "End Game"

"End Game" --> [evNewGame] "No Game"
"End Game" --> (*)

@enduml
